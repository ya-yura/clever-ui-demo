# Analytics Tracking Server Configuration
# Copy this file to .env and adjust values

# Server port
PORT=9001

# SQLite database path (relative to server directory)
DB_PATH=./analytics.db

# CORS origins (comma-separated, no spaces)
# Add your PWA domains here
CORS_ORIGINS=http://localhost:5173,http://localhost:9000

# Fallback to JSONL file storage if SQLite fails
# Set to "true" only for debugging
JSONL_FALLBACK=false

# Optional: PostHog integration (self-hosted)
POSTHOG_ENABLED=false
POSTHOG_HOST=http://localhost:8000
POSTHOG_API_KEY=your-api-key-here



